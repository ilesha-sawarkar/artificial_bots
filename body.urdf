        </visual>
        <collision>
            <origin xyz="-0.5 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.2 0.2 0.8" />
            </geometry>
        </collision>
    </link>
    <joint name="Lower_LeftLeg2" type="revolute">
